{
  "name": "webpack-boilerplate",
  "version": "3.0.1",
  "description": "Sensible webpack 5 boilerplate using Babel and PostCSS.",
  "main": "index.js",
  "author": "Tania Rascia",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "watch": "cross-env NODE_ENV=development webpack --progress --config config/webpack.watch.js",
    "lint": "eslint 'src/**/*.js' || true",
    "prettify": "prettier --write 'src/**/*.js'"
  },
  "standard": {
    "globals": [
      "THREE"
    ]
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "ejs-templates-loader": "^0.0.5",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "glslify": "^7.1.1",
    "glslify-import": "^3.1.0",
    "glslify-import-loader": "^0.1.2",
    "glslify-loader": "^2.0.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.2",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "responsive-loader": "^3.0.4",
    "sass": "^1.54.0",
    "sass-loader": "^12.2.0",
    "sharp": "^0.25.4",
    "style-loader": "^3.3.0",
    "template-ejs-loader": "^0.9.3",
    "wasm-loader": "^1.3.0",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "webpack-watch-files-plugin": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:taniarascia/webpack-boilerplate"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap": "^5.2.0",
    "camera-controls": "^1.34.4",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-noise": "0.0.0",
    "gsap": "^3.10.4",
    "is-touch-device": "^1.0.1",
    "ismobilejs": "^1.1.1",
    "load-bmfont": "^1.4.1",
    "lottie-web": "^5.9.4",
    "matter-js": "^0.18.0",
    "minivents": "^2.2.1",
    "moment": "^2.29.3",
    "msdf-bmfont-xml": "^2.6.0",
    "node-html-parser": "^5.3.3",
    "postprocessing": "^6.26.3",
    "simplex-noise": "^4.0.1",
